    script(src='javascript/client.js')
  body
    block app
    div.container
      block header
        nav.navbar.navbar-inverse
          div.container-fluid
            div.navbar-header
              a.navbar-brand(href="#")= title
                span.glyphicon.glyphicon-cd
            ul.nav.navbar-nav
              li#menu-home 
                a(href="#") Home
              li#menu-movies
                a(href="#") Movies
              li#menu-customers
                a(href="#") Customers
              li#menu-employees
                a(href="#") Employees
              li#menu-transaction
                a(href="#") Process Transaction      
              li#menu-reports
                a(href="#") Reports
            ul.nav.navbar-nav.navbar-right
              li#login
                a(href="#") Login
              li#register
                a(href="#") Register
      block content
      block footer
        div.row.container 
          p.text-center#copywrite
            